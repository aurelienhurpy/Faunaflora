{% extends 'base.html.twig' %}

{% block title %}Partenaires{% endblock %}

{% block body %}

<main>
    <div class="breadcumb-area">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="{{ path('homepage') }}">Accueil</a></li>
                <li class="breadcrumb-item active" aria-current="page">Partenaires</li>
            </ol>
        </nav>
    </div>
    <div class="clever-catagory bg-img d-flex align-items-center justify-content-center p-3" style="background-image: url(img/bg-img/bg5.jpg);">
        <div class="blog-details-headline">
            <h3>Partenaires</h3>
            <div class="meta d-flex align-items-center justify-content-center">
                
            </div>
        </div>
    </div>
    <section class="blog-area blog-page section-padding-100">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="blog-catagories mb-70 d-flex flex-wrap cat justify-content-center">
                        {% for partner in partner %}
                            <a href="{{ partner.link }}" targets"blank" class="align-self-center">
                                <div class="single-catagories bg-img">
                                    <img src="{{ asset('images/fauna/' ~ partner.fileName) }}" alt=""> 
                                </div>
                            </a>
                        {% endfor %}
                    </div>
                </div>
            </div>
        </div>
    </section>
</main>
{% endblock %}
